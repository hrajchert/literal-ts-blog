{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2015",
      /* Strict Type-Checking Options */
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "noImplicitThis": true,
      "alwaysStrict": true,
      "lib": ["es5", "es2015"],
      /* dtslint needs these to operate in this directory */
      "baseUrl": "."
    }
  }